{
  "changelog": {

    "entryTypes": [
      {
        "type": "feat",
        "DisplayName": "New Features"
      },
      {
        "type": "fix",
        "DisplayName": "Bug Fixes"
      }
    ],

    "scopes": [],

    "footers": [],

    "tagPatterns": [
      "^(?<version>\\d+\\.\\d+\\.\\d+.*)",
      "^v(?<version>\\d+\\.\\d+\\.\\d+.*)"
    ],

    "outputPath": "changelog.md",

    "integrations": {

      "provider": "none",

      "github": {
        "accessToken": null
      },

      "gitlab": {
        "accessToken": null
      }
    },

    "versionRange": null,

    "currentVersion": null,

    "template": {
      "name": "default",

      "default": {
        "markdownPreset": "default"
      }
    },

    "parser": {
      "mode" :  "loose"
    }
  }
}
